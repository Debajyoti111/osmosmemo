{
  "name": "Meta",
  "version": "1.0.0",
  "description": "Meta information about the current page",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "declarativeContent"]
}
