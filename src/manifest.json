{
  "name": "osmos::memo",
  "version": "2.1.1",
  "description": "An in-browser bookmark manager with easy tagging and fast recall",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Add memo",
    "default_popup": "popup.html",
    "default_icon": {
      "48": "assets/osmosmemo-logo-48.png",
      "96": "assets/osmosmemo-logo-96.png"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "48": "assets/osmosmemo-logo-48.png",
    "96": "assets/osmosmemo-logo-96.png"
  },
  "permissions": ["activeTab", "storage"],
  "browser_specific_settings": {
    "gecko": {
      "id": "osmosmemo@osmoscraft.org"
    }
  }
}
